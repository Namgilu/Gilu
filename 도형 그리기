void setup(){

  size(500,500); //window size 

 



}



void draw(){

   fill(255,0,0); //채우는 color 

   rect(100,100,150,150); //rect(x좌표, y좌표, 가로, 세로) 사각형

   ellipse(100, 200, 150, 150); //ellipse(x좌표, y좌표, 가로, 세로) 원

   

   fill(0,255,0); //color 

   rect(150,150,150,150);

   ellipse(200, 300, 150, 150);

   

   fill(0,0,255); //color 

   rect(200,200,150,150);

   ellipse(300, 400, 150, 150);

}

